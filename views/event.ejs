
  <body>
    <h1>Event</h1><br>
    <form class="d-flex shadow-sm p-3 mb-5 bg-body-tertiary rounded bg-info-subtle" role="search" style=" width: 70rem;margin-left: 5rem; margin-top: 2rem;">
      <input class="form-control me-2" type="search" placeholder="Search Event" aria-label="Search" name="name"> 
      <button class="btn btn-outline-primary" type="submit">Search</button>
    </form>  
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <button type="button"> <a href="/events/add">Create Event</a></button>    
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Nama</th>
            <th scope="col">Schedule</th>
            <th scope="col">Place</th>            
          </tr>
        </thead>
        <tbody>
            <% data.forEach((item) => { %>
                <tr>            
                 <td><%= item.name %></td>
                 <td><%= formatDate(item.schedule)%></td>
                 <td><%= item.place %></td>                               
                 <td><button type="button" class="btn btn-primary"> <a  style="color: aliceblue; text-decoration: none;" href="#" >Join Event</a></button></td>          
               </tr>
               <% }) %>              
        </tbody>
      </table>

</body>




